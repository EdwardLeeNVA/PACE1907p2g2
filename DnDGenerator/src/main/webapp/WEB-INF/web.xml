<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
	<servlet-name>FrontController</servlet-name>
  	<servlet-class>com.revature.dnd_generator.servlet.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>/Generator/*</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
